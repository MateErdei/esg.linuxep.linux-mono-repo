{
  "options": {
    "host_identifier": "hostname",
    "schedule_splay_percent": 10
  },
  "schedule": {
    "process_events": {
      "query": "select count(*) from process_events;",
      "interval": 86400
    },
    "user_events": {
      "query": "select count(*) from user_events;",
      "interval": 86400
    },
    "selinux_events": {
      "query": "select count(*) from selinux_events;",
      "interval": 86400
    },
    "socket_events": {
      "query": "select count(*) from socket_events;",
      "interval": 86400
    }
  }
}