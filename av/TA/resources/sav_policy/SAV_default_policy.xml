<?xml version="1.0"?>
<config xmlns="http://www.sophos.com/EE/EESavConfiguration">
    <csc:Comp xmlns:csc="com.sophos\msys\csc" RevID="ac9eaa2f09914ce947cfb14f1326b802ef0b9a86eca7f6c77557564e36dbff9a" policyType="2"/>
    <entity>
        <productId/>
        <product-version/>
        <entityInfo/>
    </entity>
    <onAccessScan>
        <enabled>false</enabled>
        <audit>false</audit>
        <scanBehaviour>
            <level>normal</level>
            <archives>false</archives>
            <pua>true</pua>
            <suspiciousFileDetection>false</suspiciousFileDetection>
            <scanForMacViruses>false</scanForMacViruses>
            <anti-rootkits>false</anti-rootkits>
            <aggressiveEmailScan>{{aggressiveEmailScan}}</aggressiveEmailScan>
        </scanBehaviour>
        <extensions>
            <allFiles>false</allFiles>
            <excludeSophosDefined/>
            <userDefined/>
            <noExtensions>true</noExtensions>
        </extensions>
        <linuxExclusions>
            <filePathSet></filePathSet>
            <excludeRemoteFiles>true</excludeRemoteFiles>
        </linuxExclusions>
        <actions>
            <disinfect>true</disinfect>
            <puaRemoval>true</puaRemoval>
            <fileAction>doNothing</fileAction>
            <destination/>
            <suspiciousFiles>
                <fileAction>doNothing</fileAction>
                <destination/>
            </suspiciousFiles>
        </actions>
        <onAccess>
            <fileRead>false</fileRead>
            <fileWrite>false</fileWrite>
            <fileRename>false</fileRename>
            <accessInfectedRMBootSectors>false</accessInfectedRMBootSectors>
            <useNetworkChecksums>false</useNetworkChecksums>
        </onAccess>
    </onAccessScan>
    <onDemandScan>
        <extensions>
            <allFiles>false</allFiles>
            <excludeSophosDefined/>
            <userDefined/>
            <noExtensions>true</noExtensions>
        </extensions>
        <posixExclusions>
            <filePathSet></filePathSet>
            <excludeRemoteFiles>true</excludeRemoteFiles>
        </posixExclusions>
        <scanSet/>
    </onDemandScan>
    <autoExclusions>{{autoExclusions}}</autoExclusions>
    <appDetection>{{appDetection}}</appDetection>
    <approvedList>{{puaExclusions}}</approvedList>
    <detectionFeedback>
        <sendData>true</sendData>
        <sendFiles>false</sendFiles>
    </detectionFeedback>
</config>