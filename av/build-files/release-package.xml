<?xml version="1.0" encoding="utf-8"?><package name="sspl-plugin-anti-virus" version="1.1.2">
    <versioning>
        <workingdir>./products/distribution/include/AutoVersioningHeaders</workingdir>
    </versioning>

    <inputs>
        <workingdir>.</workingdir>
        <!-- DIFF SEPARATION COMMENT -->
        <intermediate artifact-path="sspl-base/pluginapi" dest-dir="input" />
