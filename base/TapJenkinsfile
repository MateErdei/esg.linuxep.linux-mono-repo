#!/usr/bin/env groovy
@Library("tap-pipe")

def params = [
    string(name: 'UPSTREAM_CONTEXT',
           defaultValue: '{}',
           description: 'Information about the build which triggered this in JSON.'),
    booleanParam(name: 'INCLUDE_BUILD_IN_PIPELINE', defaultValue: false, description: 'Perform a build via the pipeline: unified builds')
]
tapPipeline { pipelineScriptPath = './pipeline/pipeline.py'; pipelineEntryPoint = 'sspl_base'; buildParameters = params }
